{
  "manifest_version": 2,

  "name": "squid",
  "description": "Allows you to open web pages on your Android device",
  "version": "1.1.0.2",

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "key": "",

  "permissions": [
    "activeTab",
    "gcm",
    "identity",
    "identity.email",
    "storage",
    "http://localhost/",
    "https://sirnommington.com/"
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "oauth2": {
    "client_id": "670316986609-htl91dqi7i612v3mq46tfu503u6gfi6d.apps.googleusercontent.com",
    "scopes": [
        "https://www.googleapis.com/auth/userinfo.email"
      ]
  },

  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_popup": "popup.html",
    "default_title": "Send to device"
  },

  "options_page": "options.html",

  "background": {
    "scripts": [
      "node_modules/systemjs/dist/system.src.js",
      "system.config.js",
      "src/background/bootstrap.js"
    ]
  }
}