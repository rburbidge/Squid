{
  "manifest_version": 2,

  "name": "squid",
  "description": "Allows you to open web pages on your phone",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Send to phone"
  },
  
  "permissions": [
    "activeTab",
    "identity",
    "https://ajax.googleapis.com/",
    "http://localhost/",
    "http://sirnommington.com/"
  ],

  "oauth2": {
    "client_id": "670316986609-htl91dqi7i612v3mq46tfu503u6gfi6d.apps.googleusercontent.com",
    "scopes": [ "https://www.googleapis.com/auth/drive"]
  },

  "background":
  {
    "scripts": ["devices.js", "lib/jquery-3.1.0.min.js"]
  }
}